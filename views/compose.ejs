<%- include('partials/header') %>
<h1>Compose</h1>
<form action="/" method="post">
      <label for="title" class="form-label">Title</label>
      <input type="text" class="form-control" id="title" name="postTitle" required>
      <label for="post" class="form-label">Post</label>
      <textarea class="form-control" id="post" rows="5" cols="30" name="postBody" required></textarea>
    <button type="submit" class="btn btn-primary post-publish" name="button">Publish</button>
  </form>

<%- include('partials/footer') %>