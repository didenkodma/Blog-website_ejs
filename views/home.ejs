<%- include('partials/header') %>

<h1>Home</h1>
<p><%= homeStarting %></p>

<% posts.forEach((post) => {   
    let link = '/posts/' + post._id;
%>

<h1><%= post.postName %></h1>
<p>
    <%= post.postValue.slice(0, 100) + " ..." %> 
    <a href=<%= link %>>Read More</a>
</p>

<% }) %>

<%- include('partials/footer') %>